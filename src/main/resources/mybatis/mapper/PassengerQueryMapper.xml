<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zwn.trainserverspringboot.query.mapper.PassengerQueryMapper">

    <select id="queryPassengerDetails" resultType="com.zwn.trainserverspringboot.command.bean.Passenger">
        SELECT passenger_id,passenger_name,phone_num
        FROM passenger_history
        WHERE user_id = #{param1}
    </select>



</mapper>